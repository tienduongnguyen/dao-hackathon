{
  "name": "mesme",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "8081": "adb -d reverse tcp:8081 tcp:8081 && yarn start",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-ios": "cd ios && xcodebuild clean && cd ..",
    "android": "yarn clean-android && npx react-native run-android ",
    "android-release": "clean-android && npx react-native run-android --variant=release",
    "format": "prettier --write **/*.{ts,tsx,js,json,md}",
    "check-code": "prettier --check **/*.{ts,tsx,js,json,md}",
    "ios": "yarn clean-ios && npx react-native run-ios",
    "start": "node ./src/assets/icon/source/test.tsx",
    "pod": "cd ios && pod install && cd ..",
    "test": "jest",
    "type:check": "yarn tsc --noEmit --skipLibCheck",
    "lint": "eslint index.js src --fix --ext .js,.ts,.tsx",
    "splash": "yarn react-native generate-bootsplash splash/splash.png \\--background-color=253754 \\ --logo-width=1000 \\--assets-path=assets \\--flavor=main",
    "gen": "node generateRes.js",
    "apk": " cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "cp_all": "yarn cp_ios && yarn cp_android",
    "postinstall": "node ./scripts/main.js",
    "notification:ios:dev": "node ./scripts/notification-ios.js env/.dev",
    "build:staging": "fastlane ios release_build --env staging && fastlane android release_build --env staging",
    "build:prod": "fastlane android release_build --env staging && fastlane ios prod_release --env prod"
  },
